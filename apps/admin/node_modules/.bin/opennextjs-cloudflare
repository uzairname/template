#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/uz/projects/template/node_modules/.pnpm/@opennextjs+cloudflare@1.8.5_wrangler@4.40.1/node_modules/@opennextjs/cloudflare/dist/cli/node_modules:/Users/uz/projects/template/node_modules/.pnpm/@opennextjs+cloudflare@1.8.5_wrangler@4.40.1/node_modules/@opennextjs/cloudflare/dist/node_modules:/Users/uz/projects/template/node_modules/.pnpm/@opennextjs+cloudflare@1.8.5_wrangler@4.40.1/node_modules/@opennextjs/cloudflare/node_modules:/Users/uz/projects/template/node_modules/.pnpm/@opennextjs+cloudflare@1.8.5_wrangler@4.40.1/node_modules/@opennextjs/node_modules:/Users/uz/projects/template/node_modules/.pnpm/@opennextjs+cloudflare@1.8.5_wrangler@4.40.1/node_modules:/Users/uz/projects/template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/uz/projects/template/node_modules/.pnpm/@opennextjs+cloudflare@1.8.5_wrangler@4.40.1/node_modules/@opennextjs/cloudflare/dist/cli/node_modules:/Users/uz/projects/template/node_modules/.pnpm/@opennextjs+cloudflare@1.8.5_wrangler@4.40.1/node_modules/@opennextjs/cloudflare/dist/node_modules:/Users/uz/projects/template/node_modules/.pnpm/@opennextjs+cloudflare@1.8.5_wrangler@4.40.1/node_modules/@opennextjs/cloudflare/node_modules:/Users/uz/projects/template/node_modules/.pnpm/@opennextjs+cloudflare@1.8.5_wrangler@4.40.1/node_modules/@opennextjs/node_modules:/Users/uz/projects/template/node_modules/.pnpm/@opennextjs+cloudflare@1.8.5_wrangler@4.40.1/node_modules:/Users/uz/projects/template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@opennextjs/cloudflare/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../@opennextjs/cloudflare/dist/cli/index.js" "$@"
fi
