{
  "$schema": "../../packages/utils/wrangler-config-schema.json",
  "name": "template-api",
  "main": "src/index.ts",
  "compatibility_date": "2025-03-01",
  "compatibility_flags": [
    "nodejs_compat",
  ],
  "dev": {
    "port": 8989
  },
  "observability": {
    "enabled": true
  },
  "env": {
    "development": {
      "name": "template-backend-dev",
      "vars": {
        "ENVIRONMENT": "development"
      }
    },
    "production": {
      "name": "template-backend", // <project_name>-backend. Must match project name in terraform/main.tf
      "route": {
        "pattern": "uzairname.org/api/*",
        "custom_domain": true
      },
      "vars": {
        "ENVIRONMENT": "production"
      },
      "r2_buckets": [
        {
          "binding": "NEXT_INC_CACHE_R2_BUCKET",
          "bucket_name": "template",
        }
      ],
    }
  }
}
